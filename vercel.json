{
    "version": 2,
    "builds": [
        {
         "src": "./index.js",
         "use": "@vercel/node"
        
        }],
    "routes": [
            {
             "src": "../src/routes",
             "dest": "/"
            }
           ],

    "rewrites": [{
            "source": "/api/(.*)",
            "destination": "/api/index.js"
          }]
}
