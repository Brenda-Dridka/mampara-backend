{
    "version": 2,
    "builds": [
        {
         "src": "./index.js",
         "use": "@vercel/node"
        
        }],
    "routes": [
            {
             "src": "../src/routes/index.js",
             "dest": "../src/routes/index.js"
            }
           ],

    "crons": [
            {
              "path": "/api/v1/every-minute",
              "schedule": "* * * * *"
            },
            {
              "path": "/api/v1/every-hour",
              "schedule": "0 * * * *"
            },
            {
              "path": "/api/v1/every-day",
              "schedule": "0 0 * * *"
            }
          ]

  
}
